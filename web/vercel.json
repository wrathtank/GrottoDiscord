{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/servers",
      "dest": "/servers.html"
    },
    {
      "src": "/panel",
      "dest": "/panel.html"
    },
    {
      "src": "/airdropper",
      "dest": "/airdropper.html"
    },
    {
      "src": "/grottle",
      "dest": "/grottle.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
